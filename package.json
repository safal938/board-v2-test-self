{
  "name": "canvas-storybook",
  "version": "1.0.0",
  "description": "A Miro-like board application with focus object feature and Storybook integration",
  "private": true,
  "dependencies": {
    "@googleworkspace/meet-addons": "^1.2.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/styled-components": "^5.1.0",
    "ajv": "^8.17.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "framer-motion": "^10.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.4",
    "react-scripts": "5.0.1",
    "react-spring": "^9.7.0",
    "redis": "^4.7.1",
    "styled-components": "^6.0.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "web-vitals": "^3.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.0",
    "@storybook/addon-interactions": "^7.5.0",
    "@storybook/addon-links": "^7.5.0",
    "@storybook/blocks": "^7.5.0",
    "@storybook/react": "^7.5.0",
    "@storybook/react-webpack5": "^7.5.0",
    "@storybook/testing-library": "^0.2.0",
    "concurrently": "^8.2.2",
    "storybook": "^7.5.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "vercel-build": "npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "server": "node api/server.js",
    "server-redis": "node api/server-redis.js",
    "server-sessions": "node api/server-redis-sessions.js",
    "dev": "concurrently \"npm run server-redis\" \"npm start\"",
    "dev-sessions": "concurrently \"npm run server-sessions\" \"npm start\"",
    "dev-file": "concurrently \"npm run server\" \"npm start\"",
    "install-server": "npm install express cors",
    "test-api": "node test-api.js",
    "test-enhanced": "node test-enhanced-todo.js",
    "test-ehr-data": "node test-ehr-data.js",
    "test-redis": "node test-redis-connection.js",
    "test-sessions": "node test-sessions.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "off",
      "no-console": "off",
      "@typescript-eslint/no-unused-vars": "off"
    },
    "ignorePatterns": ["**/*"]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "20.x"
  }
}
